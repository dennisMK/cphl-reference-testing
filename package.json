    "next": "^15.2.3",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^4.0.16",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "resend": "^4.6.0",
